{
    "meta" : {
        "id": "hyper_format",
        "label": "import_hyper",
        "description": "Import Hyper file in DSS",
        "icon": "icon-puzzle-piece"
    },

    "canBeDatasetFormat": true,
    "canRead": true,
    "canWrite": false,
    "canExtractSchema": true,

    "exportOptions": [],

    "mime" : {
        "mimeType": "text/plain",
        "extension": ".csv"
    },

    "params": [
        {
            "name": "schema_name",
            "label": "Schema name",
            "type": "STRING",
            "description": "A schema already existing in the Hyper file",
            "mandatory": true
        },
        {
            "name": "table_name",
            "label": "Table name",
            "type": "STRING",
            "description": "A table already existing in the Hyper file and associated to prefilled schema",
            "mandatory": true
        }
    ]
}

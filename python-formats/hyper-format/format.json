{
    "meta" : {
        "id": "Hyper",
        "label": "Custom format hyper-format",
        "description": "My plugin formatter",
        "icon": "icon-puzzle-piece"
    },

    "canBeDatasetFormat": true,
    "canRead": true,
    "canWrite": false,
    "canExtractSchema": true,

    "exportOptions": [
        {
            "id": "option1",
            "label": "export using this plugin",
            "config" : { "parameter1": "d" },
            "compression": "NONE"
        }
    ],

    "mime" : {
        "mimeType": "text/plain",
        "extension": ".csv"
    },

    "params": [
        {
            "name": "schema_name",
            "label": "Schema name",
            "type": "STRING",
            "description": "The name of the Schema contained in the Hyper File",
            "mandatory": true
        },
        {
            "name": "table_name",
            "label": "Table name",
            "type": "STRING",
            "description": "The name of the Table contained in the Hyper File",
            "mandatory": true
        }
    ]
}

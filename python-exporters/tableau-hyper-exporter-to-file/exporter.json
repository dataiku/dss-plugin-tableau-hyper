{
    "meta" : {
        "label": "Export to Hyper file",
        "description": "Export a DSS dataset as a Tableau Hyper file",
        "icon": "icon-puzzle-piece"
    },

    "exportBehavior" : "OUTPUT_TO_FILE",

    "mime" : {
        "mimeType": "text/plain",
        "extension": ".hyper"
    },

    "exportOptions": [
        {
            "label" : "Tableau Software: to Hyper file (.hyper)"
        }
    ],

    "params": [
        {
            "name": "schema_name",
            "label": "schema_name",
            "type": "STRING",
            "defaultValue": "dss_schema"
        },
        {
            "name": "table_name",
            "label":"table_name",
            "type": "STRING",
            "defaultValue": "dss_table"
        }
    ]
}
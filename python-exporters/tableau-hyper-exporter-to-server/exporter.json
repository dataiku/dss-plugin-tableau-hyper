{
    "meta" : {
        "label" : "Convert to Hyper file and send to Tableau server",
        "description": "Export a DSS dataset as a Tableau Hyper file, send it to Tableau server"
    },

    "exportBehavior" : "OUTPUT_TO_FILE",

    "mime" : {
        "mimeType": "text/plain",
        "extension": ".hyper"
    },

    "exportOptions": [
        {
            "label" : "Tableau Software: to Hyper Server (.hyper)"
        }
    ],

    "params": [
        {
            "name": "tableau_server_connection",
            "label": "Credentials from Tableau Server preset",
            "type": "PRESET",
            "parameterSetId": "tableau-server-connection"
        },
        {
            "name": "table_name",
            "label":"table_name",
            "type": "STRING",
            "defaultValue": "dss_table"
        },
        {
            "name": "schema_name",
            "label": "schema_name",
            "type": "STRING",
            "defaultValue": "dss_schema"
        },
        {
            "name": "project_name",
            "label": "project_name",
            "type": "STRING"
        }
    ]
}
